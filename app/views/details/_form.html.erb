<%= render('layouts/errors', :object => @detail) %>

<%= form_for [@store, @detail] do |f| %>
  <%= f.label :address %>
  <%= f.text_field :address %>
  <%= f.label :phone %>
  <%= f.text_field :phone %>
  <%= f.label :website %>
  <%= f.text_field :website %>
  <%= f.label :hours %>
  <%= f.text_field :hours %>
  <p><%= f.submit %></p>
<% end %>
